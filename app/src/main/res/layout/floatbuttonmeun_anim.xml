<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/stard_meun"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="120dp"
        android:src="@mipmap/strand_meun"
        app:backgroundTint="#00B4FF"
        app:fabSize="auto"
        app:rippleColor="#56abe4" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/photo_meun"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="60dp"
        android:src="@mipmap/photo_meun"
        app:backgroundTint="#00B4FF"
        app:fabSize="auto"
        app:rippleColor="#56abe4" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/dismiss_meun"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@mipmap/diass_meun"
        android:elevation="10dp"
        app:backgroundTint="#00B4FF"
        app:fabSize="auto"
        app:rippleColor="#56abe4" />

</RelativeLayout>